    "lint": "npm run eslint",
    "test": "mocha --reporter spec --recursive --timeout 60000",
    "unimported": "npx unimported --no-cache"
  },
  "pre-commit": {
    "run": [
      "unimported",
      
    ]
  },
  "pre-push": {
